% draw specials
\newcount \herm@mx%
\newcount \herm@my%
\def\herm@drawspecial(#1,#2)(#3,#4){%
  \herm@sx=#1%
  \herm@sy=#2%
  \herm@ex=#3%
  \herm@ey=#4%
  \qbezier(\herm@sx,\herm@sy)(\herm@sx,\herm@sy)(\herm@ex,\herm@ey)%
  \herm@dx=\herm@ex \herm@dy=\herm@ey%
  \herm@nsx=\herm@sx \multiply\herm@nsx by -1 \herm@nsy=\herm@sy \multiply\herm@nsy by -1%
  \advance\herm@dx by \herm@nsx \advance\herm@dy by \herm@nsy%
  \herm@mx=\herm@sx \advance\herm@mx by \herm@ex \divide\herm@mx by 2%
  \herm@my=\herm@sy \advance\herm@my by \herm@ey \divide\herm@my by 2%
  \ifnum\herm@dy = 0 \ifnum\herm@dx > 0 \herm@newx=6 \else \herm@newx=2 \fi%
  \else%
    \herm@dxdy=\herm@dx \multiply\herm@dxdy by 100 \divide\herm@dxdy by \herm@dy%
    \ifnum\herm@dxdy < 0 \multiply\herm@dxdy by -1 \fi%
    \ifnum\herm@dx > 0%
      \ifnum\herm@dy > 0%
        \ifnum\herm@dxdy < 41 \herm@newx=4  \else%
          \ifnum\herm@dxdy < 350 \herm@newx=5%
          \else \herm@newx=6 \fi%
        \fi
      \else%
        \ifnum\herm@dxdy < 41 \herm@newx=0 \else%
          \ifnum\herm@dxdy < 350 \herm@newx=7%
          \else \herm@newx=-1 \herm@newy=6 \fi%
        \fi
      \fi%
    \else%
      \ifnum\herm@dy > 0%
        \ifnum\herm@dxdy < 41 \herm@newx=4 \else%
          \ifnum\herm@dxdy < 350 \herm@newx=3 \else \herm@newx=2%
          \fi%
        \fi
      \else%
        \ifnum\herm@dxdy < 12 \herm@newx=0 \else%
          \ifnum\herm@dxdy < 29 \herm@newx=1 \else \herm@newx=2%
          \fi%
        \fi
      \fi%
    \fi%
  \fi%
  \herm@drawhalf(\herm@mx,\herm@my){\herm@newx}
}%
\newcount \herm@half%
\def\herm@drawhalf(#1,#2)#3{%
  \ifherm@verbatim%
    \PackageInfo{herm-pic}{half (dx=\the#1, dy=\the#2, \the#3)}%
  \fi%
  \herm@half=#3%
  \put(#1,#2){
    \ifnum\herm@half>4 \qbezier(-.5,0)(-.5,.2)(-.35,.35) \fi%
    \ifnum\herm@half<1 \qbezier(-.5,0)(-.5,.2)(-.35,.35) \fi%
    \ifnum\herm@half>5 \qbezier(-.35,.35)(-.2,.5)(0,.5) \fi%
    \ifnum\herm@half<2 \qbezier(-.35,.35)(-.2,.5)(0,.5) \fi%
    \ifnum\herm@half>6 \qbezier(0,.5)(.2,.5)(.35,.35) \fi%
    \ifnum\herm@half<3 \qbezier(0,.5)(.2,.5)(.35,.35) \fi%
    \ifnum\herm@half<4 \qbezier(.35,.35)(.5,.2)(.5,0) \fi%
    \ifnum\herm@half>0 \ifnum\herm@half<5 \qbezier(.5,0)(.5,-.2)(.35,-.35) \fi \fi%
    \ifnum\herm@half>1 \ifnum\herm@half<6 \qbezier(.35,-.35)(.2,-.5)(0,-.5) \fi \fi%
    \ifnum\herm@half>2 \ifnum\herm@half<7 \qbezier(0,-.5)(-.2,-.5)(-.35,-.35) \fi \fi%
    \ifnum\herm@half>3 \qbezier(-.35,-.35)(-.5,-.2)(-.5,0) \fi%
  }
}%

